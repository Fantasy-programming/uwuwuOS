    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "knip": "^3.6.0",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "typescript": "^5.3.2",
    "vite": "^5.0.0",
    "vitest": "^1.0.4"
