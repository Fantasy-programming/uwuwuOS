  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/utilities": "^3.2.2",
    "react-rnd": "^10.4.1",
    "clsx": "^2.0.0",
    "focus-trap-react": "^10.2.3",
    "use-sound": "^4.0.1",
    "immer": "^10.0.3",
    "zustand": "^4.4.7",
    "million": "^2.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.37",
